<Application x:Class="ProjectBiurka2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProjectBiurka2"
             StartupUri="Views/HomeView.xaml">
    <Application.Resources>
        <!--Kolory-->
        <Color x:Key="ColorPrim1">#99ccff</Color>
        <Color x:Key="ColorPrim2">#006699</Color>

        <!--Pedzel-->
        <SolidColorBrush x:Key="PedzelPrim1" Color="{StaticResource ColorPrim1}"/>
        <SolidColorBrush x:Key="PedzelPrim2" Color="{StaticResource ColorPrim2}"/>
    </Application.Resources>
</Application>
